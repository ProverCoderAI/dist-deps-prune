{
  "name": "dist-deps-prune-workspace",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo workspace for dist-deps-prune",
  "packageManager": "pnpm@10.30.1",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm --filter @prover-coder-ai/dist-deps-prune build",
    "check": "pnpm --filter @prover-coder-ai/dist-deps-prune check",
    "changeset": "changeset",
    "changeset-publish": "node -e \"if (!process.env.NPM_TOKEN) { console.log('Skipping publish: NPM_TOKEN is not set'); process.exit(0); }\" && changeset publish",
    "changeset-version": "changeset version",
    "dev": "pnpm --filter @prover-coder-ai/dist-deps-prune dev",
    "lint": "pnpm --filter @prover-coder-ai/dist-deps-prune lint",
    "lint:tests": "pnpm --filter @prover-coder-ai/dist-deps-prune lint:tests",
    "lint:effect": "pnpm --filter @prover-coder-ai/dist-deps-prune lint:effect",
    "test": "pnpm --filter @prover-coder-ai/dist-deps-prune test",
    "typecheck": "pnpm --filter @prover-coder-ai/dist-deps-prune typecheck",
    "start": "pnpm --filter @prover-coder-ai/dist-deps-prune start"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ProverCoderAI/dist-deps-prune.git"
  },
  "bugs": {
    "url": "https://github.com/ProverCoderAI/dist-deps-prune/issues"
  },
  "homepage": "https://github.com/ProverCoderAI/dist-deps-prune#readme",
  "license": "ISC",
  "pnpm": {
    "ignoredBuiltDependencies": [
      "esbuild",
      "node-pty"
    ],
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "msgpackr-extract",
      "unrs-resolver"
    ],
    "patchedDependencies": {
      "@typescript-eslint/eslint-plugin@8.55.0": "patches/@typescript-eslint__eslint-plugin@8.55.0.patch"
    }
  }
}
